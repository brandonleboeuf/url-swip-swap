{
  "manifest_version": 3,
  "name": "URL Swip-Swap",
  "version": "2.0.0",
  "description": "Quick URL switching between environments",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_icon": {
      "16": "assets/favicon.png",
      "48": "assets/favicon.png",
      "128": "assets/favicon.png"
    }
  },
  "options_page": "dist/options.html",
  "icons": {
    "16": "assets/favicon.png",
    "48": "assets/favicon.png",
    "128": "assets/favicon.png"
  }
} 